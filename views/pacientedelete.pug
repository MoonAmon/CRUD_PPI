extends layout

block content
    img(src='/images/banner-final.png' alt='Banner' style='width: 100%; height: 100%;')
    h1= title
    form(method='POST' action='/pacientes/delete')
        div.form-group
            label(for='cpf') CPF
            input.form-control(type='text' name='cpf' id='cpf' required='required' placeholder='Digite o CPF do paciente')
        button.btn.btn-primary(type='submit') Deletar
